function getCookie( name ) {
	var start = document.cookie.indexOf( name + "=" );
	var len = start + name.length + 1;
	if((!start) && (name!=document.cookie.substring(0,name.length))) return "";
	if(start == -1) return "";
	var end = document.cookie.indexOf( ';', len );
	if ( end == -1 ) end = document.cookie.length;
	return unescape(document.cookie.substring(len,end));
};


//Versione white list
function get_view_old(){
	  var com_userAgent=navigator.userAgent.toLowerCase();

	  if(com_userAgent.indexOf("firefox/0.")!=-1) return true;
	  //if(com_userAgent.indexOf("firefox/1.")!=-1) return true;
	  //if(com_userAgent.indexOf("msie 4")!=-1) return true;
	  //if(com_userAgent.indexOf("msie 5")!=-1) return true;
	  if(com_userAgent.match (/blackberry([0-9]{4,})\/4/ ) ) return true;
	  if(com_userAgent.match (/blackberry([0-9]{4,})\/5/ ) ) return true;
	  if(com_userAgent.indexOf("dolfin/1")!=-1) return true;
	  if(com_userAgent.indexOf("jasmine/0")!=-1) return true;
	  if(com_userAgent.indexOf("jasmine/1")!=-1) return true;
	  if(com_userAgent.indexOf("k-meleon/1")!=-1) return true;
	  if(com_userAgent.indexOf("midori/0")!=-1) return true;
	  if(com_userAgent.indexOf("navigator/8")!=-1) return true;
	  if(com_userAgent.indexOf("navigator/9")!=-1) return true;
	  if(com_userAgent.indexOf("ning/1")!=-1) return true;
	  if(com_userAgent.indexOf("nokia300/5")!=-1) return true;
	  if(com_userAgent.indexOf("nokia302/5")!=-1) return true;
	  if(com_userAgent.indexOf("nokia3710fold/5")!=-1) return true;
	  if(com_userAgent.indexOf("nokia7230/5")!=-1) return true;
	  if(com_userAgent.indexOf("opera mini/4")!=-1) return true;
	  if(com_userAgent.indexOf("opera mini/5")!=-1) return true;
	  if(com_userAgent.indexOf("opera mini/6")!=-1) return true;
	  if(com_userAgent.indexOf("opera mini/7")!=-1) return true;
	  if(com_userAgent.match (/symbianos\/9.*series60/ ) ) return true;
	  if(com_userAgent.indexOf("skyfire/2")!=-1) return true;
	  return false;
}

//Versione white list
function get_view_mailmobile_whitelist(){
	  var com_userAgent=navigator.userAgent.toLowerCase();

	  if(com_userAgent.indexOf("blackberry")!=-1) return true;
	  if(com_userAgent.indexOf("dolfin")!=-1) return true;
	  if(com_userAgent.indexOf("nokia300")!=-1) return true;
	  if(com_userAgent.indexOf("nokia302")!=-1) return true;
	  if(com_userAgent.indexOf("nokia3710")!=-1) return true;
	  if(com_userAgent.indexOf("nokia7230")!=-1) return true;
	  if(com_userAgent.indexOf("opera mini")!=-1) return true;
	  if(com_userAgent.indexOf("symbianos")!=-1) return true;
	  if(com_userAgent.indexOf("skyfire")!=-1) return true;
	  return false;
}


function red_mail(logged,a,b,c){
	var rollback_sso=false;
	var view_old=false;
	rollback_sso=false;
	if(rollback_sso===true){
		var url="http://liberomail.libero.it/";
	}else{
		var url="http://liberomail.libero.it/";
		var url_sso="https://login.libero.it?";
		var CK_WMAIL = getCookie('WMAIL');

	

		if (get_view_mailmobile_whitelist()) {
			url="http://m.libero.it/mail/";
			c=c+"M";
		} else { //stato smart - default
			//var hostname = getNWS_Hostname();
			var hostname = "mailbeta.libero.it";
			url="http://"+ hostname +"/cp/WindMailPS.jsp?rndPrx="+Math.random(); //stato BETA LOGGED -> redirect su mailbeta
			url=url_sso+"service_id=beta_email&ret_url="+encodeURIComponent(url);
			c=c+"S";
		}

		
		if (typeof (_rsCI) == "undefined") {			c = c + "U";		} 
		ckSrv(url,a,b,c,null);
		//location.href=url; 
		return false;
	}
};









function TSckSrv(lnk, a, b, c, target,P){
	id=parseInt(a);		
	//TSck_val=id+"|"+unescape(ar_topS[id][6]);
	if ((target !== undefined) && (P!="M")) {
		TSck_val=id+"|"+unescape(target);
		//if (true || (target !== undefined) && (target.indexOf('www.funweek.it') == -1)) {
		if ((target !== undefined) && (target.indexOf('www.funweek.it') == -1) && (target.indexOf('vitv.it') == -1)) {
			lnk="http://notizie.libero.it/";
								}
		/*
		if((P=="M") && (target !== undefined) && (target.indexOf('www.funweek.it') == -1) && (target.indexOf('vitv.it') == -1)) {
		//if (P=="M") {
			lnk="http://notizie.libero.it/magazine/";
								}
		*/
		Libero.HF.setCookie('TSck', TSck_val, { hour: 1	}, '/', '.libero.it');		
	}
	ckSrv(lnk,'HPL',b,c,null);
	return false;
}



	

