function local_events_news_live(htm,chn,n){
var cnt=0, cnt2=0, p=htm.indexOf("\"big adv_local\"></li>",cnt);
	while(p!=-1){
		htm = htm.substring(0,p+16) + local_events_news_block(chn,cnt2) + htm.substring(p+16);
		cnt=p+20;
		cnt2++;
		p=htm.indexOf("\"big adv_local\"></li>",cnt);
	}
	return htm;
}

function local_events_news_block(chn,cnt){
	var ht='',ev;
	try {
		var city=local_news[0].comune;
		var url=local_news[0].hostname;
		if((cnt==0)||(cnt==2)){
			if (typeof local_rnd[chn]=='undefined')local_rnd[chn]=Math.floor(Math.random() * local_even.length);
			//var cn=0;
			//if(cnt==2)cn=1;
			var cn;
			if(cnt==0) cn=local_rnd[chn];
			else {
				cn=local_rnd[chn]+1;
				if(cn>=local_even.length)cn=0;
			}
			ev=local_even[cn];
			ht=local_html_events;
			ht=ht.replace(new RegExp("%TITLE%","g"),local_limiter(ev.title,38));
			ht=ht.replace(new RegExp("%NEAR%","g"),local_limiter(ev.presso,24));
			ht=ht.replace(new RegExp("%ADDR%","g"),local_limiter(ev.via,24));
			ht=ht.replace(new RegExp("%ECITY%","g"),local_limiter(ev.comune['0']+" ("+ev.prov+")",24));
			ht=ht.replace(new RegExp("%MAPLINK%","g"),ev.url_mappa);
			if (ev.img=="") {
				ev.img="http://img12.iol.it/s/2013/i/no_evento.jpg";
			}
			ht=ht.replace(new RegExp("%IMG%","g"),ev.img);
			var dt=local_date_to_gm(ev.datainizio);
			ht=ht.replace(new RegExp("%SDAY%","g"),dt[2]);
			ht=ht.replace(new RegExp("%SMON%","g"),local_months[dt[1]-1]);
			var dt2=local_date_to_gm(ev.datafine);
			ht=ht.replace(new RegExp("%EDAY%","g"),dt2[2]);
			ht=ht.replace(new RegExp("%EMON%","g"),local_months[dt2[1]-1]);
			if((dt2[2]==dt[2])&&(dt[1]==dt2[1])){
				ht=ht.replace(new RegExp("%FROM%","g"),'il');
				ht=ht.replace(new RegExp("%TOCLASS%","g"),'hidden');
			} else {
				ht=ht.replace(new RegExp("%FROM%","g"),'dal');
				ht=ht.replace(new RegExp("%TOCLASS%","g"),'');
			}
		} else {
			var cn=Math.floor(Math.random() * local_news.length);
			ev=local_news[cn];
			ht=local_html_news;
			var b=ev.title[0];
			b=local_limiter(b,63);
			ht=ht.replace(new RegExp("%TITLE%","g"),b);
			if (ev.img[0]=="") {
				ev.img="http://img12.iol.it/s/2013/i/no_ultimora.jpg";
			}
			ht=ht.replace(new RegExp("%IMG%","g"),ev.img[0]);
			b=ev.body[0];
			b=local_limiter(b,82);
			ht=ht.replace(new RegExp("%BODY%","g"),b);
			var ts=ev.timestamp;
			var da=new Date(ts*1000);
			var hours="0"+da.getHours();
			var minutes="0"+da.getMinutes();
			var dt=da.getDate()+" "+local_months[da.getMonth()]+" "+da.getFullYear()+" "+hours.substr(-2,2)+":"+minutes.substr(-2,2);
			ht=ht.replace(new RegExp("%DATE%","g"),dt);
		}
		ht=ht.replace(new RegExp("%CITY%","g"),city);
		ht=ht.replace(new RegExp("%URL%","g"),url);
		ht=ht.replace(new RegExp("%LINK%","g"),ev.url);
	} catch(e) {
		ht='';
	}
	return ht;
}

function local_date_to_gm(dt){
	var p=dt.lastIndexOf('T');
	dt=dt.substring(0,p);
	var x=dt.split("-");
	return(x);
}

function local_events_news(ob, chn){
	var ee=ob.getElementsByTagName("li");
	var cnt=0;
	for(var i=0;i<ee.length;i++)if(ee[i].className.indexOf(' adv_local')>=0){
		if(ee[i].innerHTML.length<10){
			ee[i].innerHTML=local_events_news_block(chn,cnt);
		}
		cnt++;
	}
}

function local_limiter(st,le){
			if(st.length>le) {
				st=st.substr(0,le);
				var u=st.lastIndexOf(' ');
				if(u>=0) st=st.substring(0,u);
				st=st+" ...";
			}
			return st;
}
