function createCORSRequest(method, url) {
  var xhr = new XMLHttpRequest();
  if ("withCredentials" in xhr) {
    xhr.open(method, url, true);
  } else if (typeof XDomainRequest != "undefined") {
    xhr = new XDomainRequest();
    xhr.open(method, url);
  } else {
    xhr = null;
  }
  return xhr;
}

function HYP_METEO(ur) {
	var tm = new Date().getTime();
	var tq=ur+"?t="+tm;
	var i2 = document.getElementById('dynajs4');
	if (i2) i2.parentNode.removeChild(i2);
	var iddo = document.getElementById('page');
	var element = document.createElement('script');
	element.setAttribute('type','text/javascript'); 
	element.setAttribute('src',tq); 
	element.setAttribute('id','dynajs4'); 		
	iddo.appendChild(element);
}

function meteoV(){
	var mck = Libero.HF.getCookie('vlocal_city');
	var mck_ct='058091';
		if(mck){
		var mck_p=mck.indexOf('_idcity(');
		if(mck_p){
			var mck_pp=mck.indexOf(')',mck_p+8);
			var mck_ct=mck.substring(mck_p+8,mck_pp);	
		}
	}

	if ((parseInt(mck_ct.substr(0, 3))==900) && (getFup().lic==0)) {
		// not supported
		var t=document.getElementById('topmeteo');
		t.innerHTML='<a href="http://local.virgilio.it/meteo" target="_blank" onclick="return ckSrv(this,\'HPL\',\'meteo\',\'tappo\',\'_blank\')" style="display:block;width:140px;height:26px;background-repeat:no-repeat;text-transform:uppercase;font-size:10px;background-position: 0 -4px;line-height:11px;text-align:center;padding-top:44px;background-image:url(http://img12.iol.it/s/2013/i/m2/meteo-tappo.png)">Guarda il meteo<br>nella tua citt&agrave;</a>';
	} else {
		if (parseInt(mck_ct.substr(0, 3))==900) mck_ct=getFup().lic;
		HYP_METEO('http://i.plug.it/meteo/json/'+parseInt(mck_ct.substr(0, 3))+'/'+parseInt(mck_ct)+'.js');
	}
}

function meteo_prev_cb(jsonObj){

	var mck = Libero.HF.getCookie('vlocal_city');
	var mck_ct='roma';
	if(mck){
		var mck_p=mck.indexOf('_hostcity(');
		if(mck_p){
			var mck_pp=mck.indexOf(')',mck_p+10);
			var mck_ct=mck.substring(mck_p+10,mck_pp);
		}
	}
	
	var dataOggi = new Date();
	var giornoOggi = dataOggi.getDay();
	var meseOggi = dataOggi.getMonth();
	var oraOggi = dataOggi.getHours();

	var meteo_html = "";	
	if (jsonObj) {
		function formatFascia(giorno) {

			if (oraOggi > 17) {
				var fascia = [ "fascia4", jsonObj.giorno1[0].fascia4, jsonObj.giorno2[0].fascia4, jsonObj.giorno3[0].fascia4, jsonObj.giorno4[0].fascia4];						
			} else if (oraOggi > 11) {
				var fascia = [ "fascia3", jsonObj.giorno1[0].fascia3, jsonObj.giorno2[0].fascia3, jsonObj.giorno3[0].fascia3, jsonObj.giorno4[0].fascia3];			
			} else if (oraOggi > 5) {
				var fascia = [ "fascia2", jsonObj.giorno1[0].fascia2, jsonObj.giorno2[0].fascia2, jsonObj.giorno3[0].fascia2, jsonObj.giorno4[0].fascia2];			
			} else {
				var fascia = [ "fascia1", jsonObj.giorno1[0].fascia1, jsonObj.giorno2[0].fascia1, jsonObj.giorno3[0].fascia1, jsonObj.giorno4[0].fascia1];			
			}
			return fascia[giorno];
		}
		
		var	data1 = jsonObj.giorno3[0].data, prev1 = formatFascia(3), tmax1 = jsonObj.giorno3[0].tmax, tmin1 = jsonObj.giorno3[0].tmin;
		
		var ArrData1 = jsonObj.giorno1[0].data.split("/");
		var jsonData1 = new Date(ArrData1[2], (ArrData1[1]-1), ArrData1[0]);
		var giorno1 = jsonData1.getDay();
		var mese1 = jsonData1.getMonth();
		
		var ArrData2 = jsonObj.giorno1[0].data.split("/");
		var jsonData2 = new Date(ArrData2[2], (ArrData2[1]-1), ArrData2[0]);
		var giorno2 = jsonData2.getDay();
		var mese2 = jsonData2.getMonth();
		
		if ((giornoOggi == giorno1)&&(meseOggi == mese1)) {
			var	data1 = jsonObj.giorno1[0].data, prev1 = formatFascia(1), tmax1 = jsonObj.giorno1[0].tmax, tmin1 = jsonObj.giorno1[0].tmin, tperc1 = jsonObj.giorno1[0].tperc;
		} else if ((giornoOggi == giorno2)&&(meseOggi == mese2)) {
			var	data1 = jsonObj.giorno2[0].data, prev1 = formatFascia(2), tmax1 = jsonObj.giorno2[0].tmax, tmin1 = jsonObj.giorno2[0].tmin, tperc1 = jsonObj.giorno2[0].tperc;
		}		
		
		var img="http://img.plug.it/meteo/images/prev_"+prev1+".gif";
		var cta=jsonObj.comune;
		var lnk="http://"+mck_ct+".virgilio.it/meteo";
		var mobj=document.getElementById('hymet0t');
		var tobj=document.getElementById('hymet0b');
		var mobjCL=document.getElementById('hymet0CLt');
		var tobjCL=document.getElementById('hymet0CLb');
		
		var img2 = img.replace("http://img.plug.it/meteo/images/","http://img12.iol.it/s/2013/i/m2/");
		img2 = img2.replace(".gif",".png");
		mobj.style.backgroundImage="url("+img2+")";
		//mobj.style.backgroundPosition="34px 5px";
		mobj.setAttribute('href',lnk);
		//mobj.setAttribute('href','http://local.virgilio.it/meteo');
		mobj.setAttribute('title',"Meteo "+cta);
		mobj.setAttribute('target',"_blank");
		mobj.setAttribute('onclick',"return ckSrv(this,'HPL','meteo','icona','_blank')");
		mobjCL.style.backgroundImage="url("+img2+")";
		mobjCL.setAttribute('href',lnk);
		//mobjCL.setAttribute('href','http://local.virgilio.it/meteo');
		mobjCL.setAttribute('title',"Meteo "+cta);
		mobjCL.setAttribute('target',"_blank");
		mobjCL.setAttribute('onclick',"return ckSrv(this,'HPL','meteoCL','icona','_blank')");

		var ctas=cta;
		if (ctas.length>18) ctas=ctas.substr(0,17)+'...';
		tobj.innerHTML=ctas;
		tobj.setAttribute('href','http://local.virgilio.it/?ckset=force&cityRedirect=false');
		tobj.setAttribute('title','Cambia Localit');
		tobj.setAttribute('onclick',"return ckSrv(this,'HPL','meteo','citta',null)");
		tobjCL.innerHTML=ctas;
		tobjCL.setAttribute('href','http://local.virgilio.it/?ckset=force&cityRedirect=false');
		tobjCL.setAttribute('title','Cambia Localit');
		tobjCL.setAttribute('onclick',"return ckSrv(this,'HPL','meteoCL','citta',null)");
		
		//var spl=aaa.innerHTML.split('>');
		//var spl2=spl[2].split('<');
		//mobj.innerHTML=spl2[0].replace(' ','');
		//mobjCL.innerHTML=spl2[0].replace(' ','');		
		
		//mobj.innerHTML=tperc1+'&#176;';
		//mobjCL.innerHTML=tperc1+'&#176;';		
		mobj.innerHTML=tmax1+'&#176;';
		mobjCL.innerHTML=tmax1+'&#176;';		
		
		
	}	
	
	
	
}

function meteoP(){
	var a=document.getElementById('hiddenBayL');
	var aa=document.getElementById('localCity');
	var aaa=document.getElementById('localCityMeteo');
	if(a && aa && aaa){
		var imgv=a.getElementsByTagName("img");
		var img=imgv[0].getAttribute('src');
		var cta=aa.getAttribute('title');
		var lnk=aaa.getAttribute('href');
		var mobj=document.getElementById('hymet0t');
		var tobj=document.getElementById('hymet0b');
		var mobjCL=document.getElementById('hymet0CLt');
		var tobjCL=document.getElementById('hymet0CLb');
		
		var img2 = img.replace("http://img.plug.it/meteo/images/","http://img12.iol.it/s/2013/i/m2/");
		img2 = img2.replace(".gif",".png");
		mobj.style.backgroundImage="url("+img2+")";
		//mobj.style.backgroundPosition="34px 5px";
		//mobj.setAttribute('href',lnk);
		mobj.setAttribute('href','http://local.virgilio.it/meteo');
		mobj.setAttribute('title',"Meteo "+cta);
		mobj.setAttribute('target',"_blank");
		mobj.setAttribute('onclick',"return ckSrv(this,'HPL','meteo','icona','_blank')");
		mobjCL.style.backgroundImage="url("+img2+")";
		//mobjCL.setAttribute('href',lnk);
		mobjCL.setAttribute('href','http://local.virgilio.it/meteo');
		mobjCL.setAttribute('title',"Meteo "+cta);
		mobjCL.setAttribute('target',"_blank");
		mobjCL.setAttribute('onclick',"return ckSrv(this,'HPL','meteoCL','icona','_blank')");

		var ctas=cta;
		if (ctas.length>18) ctas=ctas.substr(0,17)+'...';
		tobj.innerHTML=ctas;
		tobj.setAttribute('href','http://local.virgilio.it/?ckset=force&cityRedirect=false');
		tobj.setAttribute('title','Cambia Localit');
		tobj.setAttribute('onclick',"return ckSrv(this,'HPL','meteo','citta',null)");
		tobjCL.innerHTML=ctas;
		tobjCL.setAttribute('href','http://local.virgilio.it/?ckset=force&cityRedirect=false');
		tobjCL.setAttribute('title','Cambia Localit');
		tobjCL.setAttribute('onclick',"return ckSrv(this,'HPL','meteoCL','citta',null)");
		var spl=aaa.innerHTML.split('>');
		var spl2=spl[2].split('<');
		mobj.innerHTML=spl2[0].replace(' ','');
		mobjCL.innerHTML=spl2[0].replace(' ','');
	}	
}
