<div align="center">
<script type="text/javascript">

	var SEC_HEAD=0;

	function HYP_SCR(nam,alt,chc_id) {
		var self=this,han=false,tmov=-1,dx=0,ddx=0,x=0,curr=0,autot=-1,pre=0,orgxx,stt,mlk=0,tot=0,w=0,li,hw=false,tob=false,chc=[],imd=0;
		if (typeof alt=='undefined') var alt=0;
		if (typeof chc_id=='undefined') var chc_id=false;

		var requestAnimationFrame=window.requestAnimationFrame||window.mozRequestAnimationFrame||window.webkitRequestAnimationFrame||window.msRequestAnimationFrame;
		if(typeof requestAnimationFrame=='undefined')requestAnimationFrame=false;
		window.requestAnimationFrame=requestAnimationFrame;
		
		this.mgo=function(m){
			if(m=='next')this.go('r');
			else this.go('l');
		}
		this.go=function(n,au,i){
			if (tmov!=-1) return;
			if (typeof au=='undefined') au=0;
			if (typeof i=='undefined') i=0;	
			if (au==0) this.auto(0);
			if (n=='l') n=curr-1;
			if (n=='r') n=curr+1;
			if (curr==n) return;
			ddx=n;
			dx=-w - (w*n);
			//this.preload();
			stt=new Date().getTime();
			if (i==1){stt=stt-330;imd=1}
			else imd=0;
			orgxx=x;
			if (tmov==-1) {
				if(hw){
					tmov=-3;
					if(i==0)setTimeout(function(){self.hw_move()},0)
					else setTimeout(function(){self.hw_callback()},0)
				} else {
					if (!requestAnimationFrame){tmov=setInterval(function(){self.move()},16)}
					else{tmov=-2;requestAnimationFrame(function(){self.move()})}
				}
			}
		}
		this.hw_move=function(){
			han.style.webkitTransition=han.style.transition='0.330s ease-in-out';
			han.style.webkitTransform=han.style.transform="translate3d("+dx+"px,0,0)";
		}
		this.hw_callback=function(){
			curr=ddx;
			if (ddx<=-1) curr=tot-1;
			if (ddx>=tot) curr=0;
			x=-w-(w*curr);
			if((curr!=ddx)||(imd==1)){
				han.style.webkitTransition=han.style.transition='0s';
				han.style.webkitTransform=han.style.transform="translate3d("+x+"px,0,0)";
			}
			imd=0;
			tmov=-1;this.hilite();this.preload();
		}
		this.move=function(){
			if (mlk==1)return;
			mlk=1;
			var dt=330-(new Date().getTime()-stt);
			if (dt>330) dt=330;if (dt<=0) dt=0;
			var att=(Math.PI*(dt/330))+(Math.PI/2);
			var dm=(Math.sin(att)+1)/2;
			x=orgxx+((dx-orgxx)*dm);
			if(dt==0)x=dx;
			han.style.left=parseInt(x)+'px';
			if (x==dx) {
				curr=ddx;
				if (ddx<=-1) curr=tot-1;
				if (ddx>=tot) curr=0;
				x=-w - (w*curr);
				han.style.left=parseInt(x)+'px';
				if(!requestAnimationFrame)clearInterval(tmov);
				tmov=-1;
				this.hilite();
				this.preload();
			} else if(requestAnimationFrame)requestAnimationFrame(function(){self.move()});
			mlk=0;
		}
		this.setup=function(){
			han = document.getElementById(nam+'x');
			pre=0;li=[];tot=0;
			for(var i in han.childNodes)if(typeof han.childNodes[i].innerHTML != 'undefined'){
					li[tot++]=han.childNodes[i];
					if(han.childNodes[i].offsetWidth>0)w=han.childNodes[i].offsetWidth;
					if (han.childNodes[i].className.indexOf('selected')>-1) curr=tot-1;
					han.childNodes[i].style.display='block';
			}
			if(chc_id!=false){
				if(typeof(chc[chc_id])=='undefined'){
					if(typeof local_events_news=='function') local_events_news(han,chc_id);
					chc[chc_id]=han.innerHTML;
				}
			}
			var lano = li[tot-1].cloneNode(true);
			var fino = li[0].cloneNode(true);
			han.insertBefore(lano,li[0]);
			han.appendChild(fino);
			li[-1]=lano;
			li[tot]=fino;
			x=-w - (w*curr);
			han.style.width=((tot+2)*w+10)+'px';
			hw=this.hw_detection();
			if(hw===false)han.style.left=x+'px';
			else{han.style.webkitTransition=han.style.transition='0s';han.style.webkitTransform=han.style.transform="translate3d("+x+"px,0,0)"}
			var o=document.getElementById(nam+'_l');
			if (o) { o.style.display='block';addEvent(o,'click',function(){self.go('l');return false;}) }
			o=document.getElementById(nam+'_r');
			if (o) { o.style.display='block';addEvent(o,'click',function(){self.go('r');return false;}) }

			if(document.getElementById(nam+'_k')){
				var ooo=document.getElementById(nam+'_k');
				if(!document.getElementById(nam+'_k0')){
					for(i=0;i<tot;i++){
						var a=document.createElement('a');a.id=nam+'_k'+i;a.href='#';
						ooo.appendChild(a);
					}
				}
			}
			for(i=0;i<tot;i++) {
				o=document.getElementById(nam+'_k'+i);
				if (o) {
					o.idx=i;
					if (alt!=1) {
						o.onclick=function(){return false};
						addEvent(o,'click',function(e){e=e||window.event;var o=this;if(e.srcElement)o=e.srcElement;self.go(o.idx);return false;})
					}
					if (curr!=i) o.className="";
					else o.className="selected"
				}
			}
			mlk=0;
			this.hilite();
			tob=new HYP_TCH(han,function(g){self.mgo(g)});
			if(hw!==false)han.className='hwslider';
			if(hw)han.addEventListener(hw,function(){self.hw_callback()},false);
		}
		this.initload=function() {
			preLoad(li[curr]);
			preLoad(li[curr+1],'next');	
		}
		this.preload=function() {
			if (pre==1) return;
			preLoad(han);
			pre=1;
		}
		this.hilite=function() {
			for(i=0;i<tot;i++) {
				o=document.getElementById(nam+'_k'+i);
				if (o) {
					if (curr!=i) o.className="";
					else o.className="selected"
				}
			}
		}
		this.auto=function(t) {
			if (autot!=-1) clearInterval(autot);
			autot=-1;
			if (t>0) {
				autot=setInterval(function(){self.go('r',1)},t*1000);
			}
		}	
		this.load=function(o,c,n){
			if(tmov!=-1)return;
			tmov=-5;
			var ooo=document.getElementById(nam+'_k');
			if(ooo)ooo.innerHTML='';
			han.style.display='none';
			if(typeof(chc[c])!='undefined') {
				setTimeout(function(){self.inject(chc[c],c,n)},1);
				return;
			}
			if(hw===false)han.style.left="0";
			else{han.style.webkitTransition=han.style.transition='0s';han.style.webkitTransform=han.style.transform=""}
			han.style.display='block';
			han.innerHTML='<li class="loading"></li>';
			var tm = new Date().getTime();
			var tq="/post_slider.php?c="+c+"&s="+o+"&n="+n+"&t="+tm;
			var i2 = document.getElementById('dynajs3');
			if (i2) i2.parentNode.removeChild(i2);
			var iddo = document.getElementById('page');
		  var element = document.createElement('script');
			element.setAttribute('type','text/javascript'); 
			element.setAttribute('src',tq); 
			element.setAttribute('id','dynajs3'); 		
			iddo.appendChild(element);
			ckSrv(document.location.href,'HPL','tabslider',c,'_ajax');
		}
	
		this.inject=function(c,cc,n){
				var inchc=true;
				var poo=han.parentNode;
				poo.removeChild(han);
				var oo=document.createElement('div');
				oo.setAttribute('id',nam+'x');
				poo.appendChild(oo);
				if(typeof(chc[cc])=='undefined'){
					if (typeof local_events_news_live == 'function') {
						c=local_events_news_live(c,cc,n);
					}
				}
				oo.innerHTML=c;
				if(typeof(chc[cc])=='undefined'){chc[cc]=c;inchc=false}
				this.setup();
				if(!inchc)this.initload();
				else this.preload();
				for(var i=0;i<16;i++){
					var o=document.getElementById(nam+'_mk'+i);
					if (o) {
						if(i==n)o.className='selected';
						else o.className='';
					}
				}
				tmov=-1;
		}
		this.hw_detection=function(){
			if(typeof(opera)!='undefined'){
				if(opera.version()<14)return false;
			}
			//if(navigator.userAgent.match(/Android/))if(!(navigator.userAgent.match(/(Chrome|Opera)/)))return false;
			//if(navigator.userAgent.match(/Linux/))return false;
			var el = document.createElement('fakeelement');
			var transitions = {
				'WebkitTransition':'webkitTransitionEnd',
				'transition':'transitionend'
			}
			for(var t in transitions)if( el.style[t] !== undefined )return(transitions[t]);
			return false;
		}
}
		
function HYP_TCH(obj,cabe) {
	var self=this,sx,sy,app=0,evt='';
	var supp=("ontouchstart" in document.documentElement);
	if(supp)if(navigator.userAgent.match(/(iPod|iPhone|iPad)/) && navigator.userAgent.match(/AppleWebKit/)) app=1;
	if(supp)obj.addEventListener('touchstart', function(e){self.tstart(e)}, false);

	this.tstart=function(e){
      sx=e.touches[0].pageX;sy=e.touches[0].pageY;
			evt='';
      obj.addEventListener('touchmove', function(e){self.tmove(e)}, false);
      obj.addEventListener('touchend', function(e){self.tend(e)}, false);
	}
	this.tmove=function(e){
		if(evt=='scroll')return;
    if(e.touches.length>1)return;
    var dx=e.touches[0].pageX - sx,dy=e.touches[0].pageY - sy;
    if (evt=='')if(Math.abs(dx)<Math.abs(dy))evt='scroll';
    if (evt!='scroll') {
    	e.preventDefault();
     	if(evt!='swipe'){
				if(Math.abs(dx)>30){
					evt='swipe';
					if(dx>0)cabe('prev');
					else cabe('next');
				}	
			}
   	}
	}
	this.tend=function(e){
		evt='';
    obj.removeEventListener('touchmove', function(e){self.tmove(e)}, false);
    obj.removeEventListener('touchend', function(e){self.tend(e)}, false);
	}
}
	
function HYP_LIVE(ur) {
	var self=this,mode=0,xx,sep,lb,tm=-1;
	this.ob=false;this.shortwait=60;this.longwait=600;this.ur=ur;
	this.set=function(ht) {
		if (typeof ht=='undefined') ht='';
		xx=document.getElementById('liveboxx');
		if(ht=='auto'){ht=xx.innerHTML;if(ht.length<10)ht=''}
		this.ob=false;
		xx.innerHTML=ht;
		if (tm!=-1) clearTimeout(tm);
		tm=-1;
		if (ht=='') {
			this.whattime();
			this.showhide(0);
			tm=setTimeout(function(){self.update()},this.longwait*1000);
			return;
		}
		xx.innerHTML=ht;
		this.showhide(1);
		this.ob=new HYP_SCR('livebox',1);
		this.ob.setup();	
		this.ob.preload();
		tm=setTimeout(function(){self.update()},this.shortwait*1000);
	}
	this.update=function() {
			var tm = new Date().getTime();
			var tq=this.ur+"&t="+tm;
			var i2 = document.getElementById('dynajs2');
			if (i2) i2.parentNode.removeChild(i2);
			var iddo = document.getElementById('page');
		  var element = document.createElement('script');
			element.setAttribute('type','text/javascript'); 
			element.setAttribute('src',tq); 
			element.setAttribute('id','dynajs2'); 		
			iddo.appendChild(element);
	}
	this.showhide=function(m){
		if (mode==m) return;
		sep=document.getElementById('liveseparator');
		lb=document.getElementById('livebox');
		if(m==0) {sep.style.display='block';lb.style.display='none';banMover.move('closelive')}else{sep.style.display='none';lb.style.display='block';banMover.move('openlive')}
		mode=m;
	}
	this.whattime=function(){
		this.longwait=600;
	}
}	

function Y_SCROLL_WIDTH() {
	var myLeft = 434;
	if (window.innerWidth) {
		if (window.innerWidth<970) myLeft = 208;
		else myLeft = ((window.innerWidth - 1440)/2)+434;
		document.getElementById('suggco0').style.left = myLeft+'px';
	}
	return myLeft;
}

function Y_SCROLL_RSZ() {
	if (SEC_HEAD==1) {
		document.getElementById('suggco0').style.left = Y_SCROLL_WIDTH()+'px';
	}
}
function Y_SCROLL() {
	var d = document.documentElement,b = document.body;
	var t = (d && d.scrollTop  || b && b.scrollTop  || 0);
	if (typeof myHeadBlock=='undefined') myHeadBlock = 0;
	myHeightCL=180;
	if (document.getElementById('theader').style.height == '270px') {
		myHeightCL=290;
	} 
	if (document.getElementById('theader').style.height == '0px') {
		myHeightCL=291;
	}
	if (t>myHeightCL) {
	
		document.getElementById('suggco0').style.left = Y_SCROLL_WIDTH()+'px';
		
		document.getElementById('theader').className = '';
		document.getElementById('suggco0').style.position = 'fixed';
		document.getElementById('suggco0').style.top = '95px';
		
		document.getElementById('suggco0').style.zIndex = '10000';
		
		SEC_HEAD=1;
		document.getElementById('theaderFix').style.display="block";
		try {
			if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {	
				// do nothing
			} else if ( (!document.all) && (!(!!navigator.userAgent.match(/Trident.*rv[ :]*11\./))) ) {
				// on IE10 or older and IE11 we don't change focus
				document.getElementById('top-queryCL').focus(); 
			}
		} 
		catch (e) {}

	} else {

		SEC_HEAD=0;
		document.getElementById('theaderFix').style.display="none";
		try {
			if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {	
				// do nothing
			} else if ( (!document.all) && (!(!!navigator.userAgent.match(/Trident.*rv[ :]*11\./))) ) {
				// on IE10 or older and IE11 we don't change focus
				document.getElementById('top-query').focus();
			}
		}
		catch (e) {}

		document.getElementById('theader').className = '';
		document.getElementById('suggco0').style.position = 'absolute';
		if (myHeightCL==290) {
			document.getElementById('suggco0').style.top = '162px';
			document.getElementById('suggco0').style.left = '16px';
		} else if (myHeightCL==291) {
			document.getElementById('suggco0').style.top = '-110px';
			document.getElementById('suggco0').style.left = '16px';
		} else {
			document.getElementById('suggco0').style.top = '80px';
			document.getElementById('suggco0').style.left = '270px';
		}
		myHeadBlock = 0;
	}
	return t;
}
if ( /MSIE\s+(6|5|4)\./.test(navigator.appVersion) || Libero.Browser.isMobile || Libero.Browser.isTablet ) {
	// non si attiva header bloccato
} else {
	addEvent(window,'scroll',function(){Y_SCROLL()});
	addEvent(window,'resize',function(){Y_SCROLL_RSZ()});
}

