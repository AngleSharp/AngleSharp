}

	
function HYP_SUG(div) {
	var self=this,attsugg=-1,suggs=1,oldgorgot=0,suggtot=0,oldsugg='',suggstat=0;
	this.sugg_start=function(){
    if(Libero.HF.getCookie('auto_suggest')=='false')suggs=0;
    suggs=1; //Always on
    addEvent(document.hfrm.query,'keydown',function(e){self.sugg_ch(e)})
    addEvent(document.hfrmCL.query,'keydown',function(e){self.sugg_ch(e)})
    addEvent(window,'click',function(e){self.sugg_clkk(e)})
    addEvent(document,'click',function(e){self.sugg_clkk(e)})
	} 
	this.sugg_ch=function(evt){
		if (suggs==1) {
			evt=(evt)?evt:((window.event)?event:null);
			if (oldgorgot!=0)clearTimeout(oldgorgot);
			var kc=evt.keyCode;
			if ((kc==27)&&(suggstat==1)) { this.sugg_cls(); return false; }
			if ((kc==38)&&(suggstat==1)) { var n=attsugg-1; if (n>=0) this.sugg_ovr(n);if(evt.preventDefault)evt.preventDefault();return false;
			} else if ((kc==40)&&(suggstat==1)) { var n=attsugg+1; if ((n<suggtot.length)&&(n<5)) this.sugg_ovr(n); return false;
			} else oldgorgot = setTimeout(function(){self.sugg_ok()},1000);
		}
	}
	this.sugg_cls=function(){var md=document.getElementById('suggco0');md.style.display="none";this.sugg_ovr(-1);suggstat=0}
	this.sugg_ovr=function(n) {
		if (attsugg==n) return;
		if (attsugg!=-1) {var ii = document.getElementById('sugg'+attsugg);ii.className="line";}
		if (n!=-1) {var ii = document.getElementById('sugg'+n);ii.className="line sel";document.hfrmCL.query.value = suggtot[n]['value'];document.hfrm.query.value = suggtot[n]['value'];document.hfrmCL.regione.value = 1004;document.hfrm.regione.value = 1004;}
				attsugg=n;
	}
	this.sugg_clk=function(n) { document.hfrm.submit(); }
	this.sugg_ok=function(){
		var q='';
		if(SEC_HEAD==0) {q = document.hfrm.query.value;document.hfrmCL.query.value=q}
		else {q = document.hfrmCL.query.value;document.hfrm.query.value=q}
		if ((oldsugg==q)&&(suggstat==1)) return;
		if (q==''){
			this.sugg_cls();
			return;
		}
		oldsugg=q;
		if (q!='') {
			var tm = new Date().getTime();
			var tq="http://api.arianna.libero.it/suggest/abin/suggest?ts="+tm+"&method=json&enc=latin1&fnn=as&in="+escape(q);
			this.sugg_request(tq);
		}
	}
	this.sugg_request=function(ur) {
			var i2 = document.getElementById('dynajs');
			if (i2) i2.parentNode.removeChild(i2);
			var iddo = document.getElementById('page');
		  var element = document.createElement('script');
			element.setAttribute('type','text/javascript'); 
			element.setAttribute('src',ur); 
			element.setAttribute('id','dynajs'); 		
			iddo.appendChild(element);
	}
	this.suggclk=function(n){document.hfrm.submit();}
	this.sugg_clkk=function(evt) { if (suggstat==1) this.sugg_cls();  }
	this.mngSucc=function(stuff){
		var i=0;
		suggtot = stuff.results;
		var md = document.getElementById('suggco0');
		if (stuff.results.length==0) { md.style.display="none"; suggstat=0; return; }
			var nsugg = this.sugg_trim(oldsugg);
		for(i=0;((i<stuff.results.length)&&(i<5)); i++) { 
			var tota = suggtot[i]['value'];
			if (35<tota.length) tota = tota.substr(0,35)+"...";
			var stri = "<span class='evi'>"+nsugg+"</span>"+tota.substr(nsugg.length);
			var ii = document.getElementById('sugg'+i); ii.innerHTML = stri; ii.style.display="block"; }
		while (i<5) { var ii = document.getElementById('sugg'+i); ii.style.display="none"; i++; }
		this.sugg_ovr(-1); md.style.display="block"; suggstat=1;
		/*var psx=getPosition(document.getElementById('searchole'));
		if (psx.top>0) md.style.top=(psx.top+53)+'px';*/
	}
	this.sugg_trim=function(st) { return st.replace(/^\s\s*/,"").replace(/\s\s*$/,''); }
	this.sugg_ena=function(m){
	    if (suggs==m)return;
	    this.sugg_cls();
	    suggs = m;
	} 
}

function sugg_ena(m){
	//as.sugg_ena(m);//Always on
} 



function HYP_PD(imd){
	var self=this,opnd=[],tmr=[],clk=0;
	if(typeof imd=='undefined')imd=false;
	addEvent(document.body,'click',function(){if(clk==0)self.closeall();clk=0})
	this.add=function(i){
		var a=document.getElementById(i+'-t');
		var ao=document.getElementById(i);
		if(a){
			a.setAttribute('data-tid',i);
			addEvent(a,'click',function(e){e=e||window.event;var o=this;if(e.srcElement)o=e.srcElement;self.toggle(o)});
		}
		addEvent(ao,'click',function(){clk=1})
		tmr[i]=-1;
		opnd[i]=0;
	}
	this.toggle=function(o,onoff){		
		var ii=o.getAttribute('data-tid');
		if(tmr[ii]!=-1)return;
		var oo=document.getElementById(ii);
		if(oo.style.display=='block'){
			this.closeall();
		} else {
			this.closeall();
			this.open(ii);
			clk=1;
		}
	}
	this.close=function(i){
		var oo=document.getElementById(i);
		opnd[i]=0;
		if(!imd){
			oo.style.overflow='hidden';
			var h=oo.clientHeight;
			if(tmr[i]!=-1)clearTimeout(tmr[i]);
			self.move(oo,h,0,i);
		} else {
			oo.style.display='none';
		}
	}
	this.open=function(i){
		var oo=document.getElementById(i);
		oo.style.display='block';
		opnd[i]=1;
		if(!imd){
			var h=oo.clientHeight;
			oo.style.overflow='hidden';
			if(tmr[i]!=-1)clearTimeout(tmr[i]);
			self.move(oo,0,h,i);
		}
	}
	this.move=function(o,s,f,i){
		if(s!=f){
			s=Math.floor((s+f)/2);
			if(s<f)s=s+1;
			else s=s-1;
			o.style.height=s+'px';
			if(Math.abs(s-f)<2)s=f;
			tmr[i]=setTimeout(function(){self.move(o,s,f,i)},30);
		}else{
			o.style.height='auto';
			if(f==0){o.style.display='none'}
			tmr[i]=-1;
		}
	}
	this.closeall=function(){
		for(var i in opnd){
			if(opnd[i]==1){
				this.close(i);
			}
		}
	}
}
hypd=new HYP_PD();
hypd.add('pd-mailplus');hypd.add('pd-mailpec');hypd.add('pd-apriti');hypd.add('pd-blog');



function preLoad(obj,cls) {
	if (typeof cls=='undefined') cls='';
	var e=obj.getElementsByTagName("img");
	for(var i=0;i<e.length;i++)if(e[i].src.indexOf('dummy.gif')>=0)if(e[i].getAttribute)if(e[i].getAttribute("data-src")){
		var sw='';
		if(cls=='')sw=e[i].getAttribute("data-src");
		else if(e[i].className.indexOf(cls)>=0)sw=e[i].getAttribute("data-src");
		if (sw) e[i].src=sw;
	}
}

function addEvent(obj, evType, fn) {
    if (obj.addEventListener) {
        obj.addEventListener(evType, fn, false);
        return true;
    } else if (obj.attachEvent) {
        var r = obj.attachEvent("on" + evType, fn);
        return r;
    }
}


function getPosition(elem) {
    var box = elem.getBoundingClientRect()
    var body = document.body
    var docElem = document.documentElement
    var scrollTop = window.pageYOffset || docElem.scrollTop || body.scrollTop
    var scrollLeft = window.pageXOffset || docElem.scrollLeft || body.scrollLeft
    var clientTop = docElem.clientTop || body.clientTop || 0
    var clientLeft = docElem.clientLeft || body.clientLeft || 0
    var top  = box.top +  scrollTop - clientTop
    var left = box.left + scrollLeft - clientLeft
    return { top: Math.round(top), left: Math.round(left) }
}

function safeOver(obj,n) {
	if (typeof obj != 'undefined') obj.go(n,0,1);
}
